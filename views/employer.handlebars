<div class="divider"></div>

<div class="container">
    <div class="section">
        <div class="row">
            <div class="col s12">
                <div class="section">
                    <div class="col s12">
                        <span class="title">Code Hunters</span>
                    </div>
                    {{#each employer}}
                    <div class="col s12 m6 l4">
                        <div class="card blue darken-4 hoverable large white-text">
                            <div class="card-content white-text">
                                <span class="card-title">{{first_name}} {{last_name}}</span>
                                <div class="divider"></div>
                                <br>
                                <p>Business: {{business_name}}</p>
                                <div class="divider"></div>
                                <br>
                                <p>Project Details: {{project_details}}</p>
                            </div>
